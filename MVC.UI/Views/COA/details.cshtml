
@model Web.Dto.Master_COADbo

@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>Data Master COA</h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
        <li class="active">Master COA</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Detail Data COA</h3>
        </div>
        <div class="box-body">

            @using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)


                <div class="form-group">
                    <div class="col-sm-2 control-label">Kode COA</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.kode_COA, new { @class = "form-control", placeholder = "Nama COA", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.kode_COA)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Nama COA</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.nama_COA, new { @class = "form-control", placeholder = "Nama COA", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.nama_COA)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">D/R</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.DRCR, new { @class = "form-control", placeholder = "D/C", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.DRCR)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Kelompok</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.nama_kelompok_COA, new { @class = "form-control", placeholder = "Kelompok", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.nama_kelompok_COA)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Judul Level 1</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.nama_sub_gl, new { @class = "form-control", placeholder = "Sub GL", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.nama_sub_gl)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Judul Level 2</div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.nama_level_COA, new { @class = "form-control", placeholder = "Level", disabled = "disabled" })
                        @Html.ValidationMessageFor(model => model.nama_level_COA)
                    </div>
                </div>

            }

            <div>
                <a href="@Url.Action("Index")"><i class="fa fa-reply"></i>&nbsp;&nbsp;Kembali</a>
            </div>

        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.box -->
</section>
<!-- /.content -->