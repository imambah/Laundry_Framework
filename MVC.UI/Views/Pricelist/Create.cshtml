@model Web.Dto.Master_PricelistDbo

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>Data Daftar Harga</h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Daftar Harga</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Tambah Daftar Harga</h3>
        </div>
        <div class="box-body">

            @using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)

                <div class="form-group">
                    <div class="col-sm-2 control-label">Type</div>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(model => model.type, new { @class = "form-control", placeholder = "Type" })
                        @Html.ValidationMessageFor(model => model.type)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Service</div>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(model => model.service, new { @class = "form-control", placeholder = "service" })
                        @Html.ValidationMessageFor(model => model.service)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2 control-label">Laundry</div>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(model => model.laundry, new { @class = "form-control", placeholder = "Harga Laundry " })
                        @Html.ValidationMessageFor(model => model.laundry)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-2 control-label">Dry Clean</div>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(model => model.dry_clean, new { @class = "form-control", placeholder = "Harga Dry Clean" })
                        @Html.ValidationMessageFor(model => model.dry_clean)
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-2"></div>
                    <div class="col-sm-10">
                        <button class="btn btn-success"><i class="fa fa-save"></i>&nbsp;&nbsp;Simpan</button>
                    </div>
                </div>
            }

            <div>
                <a href="@Url.Action("Index")"><i class="fa fa-reply"></i>&nbsp;&nbsp;Kembali</a>
            </div>

        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.box -->
</section>
<!-- /.content -->